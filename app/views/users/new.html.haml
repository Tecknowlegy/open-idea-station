#user
  %section.user-form
    .title
      %h6 Create Your Account
    .body
      =render "signup_form", user: @user
    .services-divider
      .bar
      .service-title Use external service
    .external-services
      .services
        = link_to "/auth/google_oauth2", class: "signup google" do
          %i.fa.fa-google
        = link_to "/auth/github", class: "signup github" do
          %i.fa.fa-github
